{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/presentation.html"
    },
    {
      "source": "/banque",
      "destination": "/banque.html"
    },
    {
      "source": "/comptes",
      "destination": "/comptes.html"
    },
    {
      "source": "/paiements",
      "destination": "/paiements.html"
    },
    {
      "source": "/credits",
      "destination": "/credits.html"
    },
    {
      "source": "/assurance",
      "destination": "/assurance.html"
    },
    {
      "source": "/entreprises",
      "destination": "/entreprises.html"
    },
    {
      "source": "/projets-de-vie",
      "destination": "/projets-de-vie.html"
    },
    {
      "source": "/presentation",
      "destination": "/presentation.html"
    },
    {
      "source": "/tutos-videos",
      "destination": "/tutos-videos.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/Sources%20Image/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/AFFICHES%20BANQUE%20PAYS/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

